<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_validate_cpe" model="ir.ui.view">
        <field name="name">view.company.form.validate.cpe</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form" />
        <field name="arch" type="xml">
            <xpath expr="//field[@name='id_token_sunat']" position="before">
                <field name="username_sunat"/>
                <field name="password_sunat"/>
            </xpath>
        </field>
    </record>
</odoo>