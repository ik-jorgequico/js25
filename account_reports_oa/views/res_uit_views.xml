<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_res_uit_form" model="ir.ui.view">
        <field name="name">view.res.uit.form</field>
        <field name="model">res.uit</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <field name="name" readonly="1" force_save="1"/>
                    <field name="year"/>
                    <field name="amount"/>
                </group>
            </form>
        </field>
    </record>
</odoo>
